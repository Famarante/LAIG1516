		</NODE>
		
		<NODE id="TABLELEG2">      

			<MATERIAL id="null" />
			
			<TEXTURE id="null" />
			
			<TRANSLATION x="3.5" y="0.0" z="-2.0" />
			<ROTATION axis="x" angle="90"/>							
			<DESCENDANTS>
				<DESCENDANT id="2" />			
			</DESCENDANTS>

		</NODE>
		
		<NODE id="TABLELEG3">      

			<MATERIAL id="null" />
			
			<TEXTURE id="null" />
			
			<TRANSLATION x="-3.5" y="0.0" z="2.0" />
			<ROTATION axis="x" angle="90"/>							
