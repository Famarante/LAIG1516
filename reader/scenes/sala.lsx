			<MATERIAL id="null" />
			
			<TEXTURE id="wood" />
			<TRANSLATION x="-8.0" y="-4.95" z="-3" />
			<SCALE sx="6.0" sy="0.2" sz="4.0" />
			
			<ROTATION axis="x" angle="-90.0" />
						
			<DESCENDANTS>
				<DESCENDANT id="FACE1" />
				<DESCENDANT id="FACE2" /> 
				<DESCENDANT id="FACE3" /> 
				<DESCENDANT id="FACE4" /> 
				<DESCENDANT id="FACE5" /> 
				<DESCENDANT id="FACE6" /> 
